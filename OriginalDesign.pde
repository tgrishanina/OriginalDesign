void setup(){
  size(400,400);
}
int y = 0;
void draw(){
  noStroke();
  fill(0,255,0);
  background(255);
  rect(200,0,10,y);
  bezier(205,y-10,180,10+y,190,25+y,205,60+y);
  bezier(205,y-10,230,10+y,220,25+y,205,60+y);
  if(y>120){
   bezier(80,200-30,90,100-30,130,180-30,200,150-30);
 }
 if(y>170){
 bezier(80,200+20,90,100+20,130,180+20,200,150+20);
 }
 if(y>200){
 bezier(320,200+50,320,100+50,280,180+50,210,150+50);
 }
 if(y>80){
 bezier(320,200-70,320,100-70,280,180-70,210,150-70);
 }
  y = y + 1;}

